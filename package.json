{
  "name": "gatsby-plugin-modern-react-i18next",
  "version": "0.1.0",
  "description": "Gatsby plugin that provides i18n support with react-i18next@v10",
  "main": "index.js",
  "author": "SerDimoa",
  "license": "MIT",
  "keywords": [
    "react",
    "gatsby",
    "gatsby-plugin",
    "gatsby-component",
    "i18next",
    "i18n"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/serdimoa/gatsby-plugin-react-i18next-v10.git"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "build:watch": "babel -w src --out-dir . --ignore __tests__",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@wapps/gatsby-i18n": "^1.1.3",
    "i18next": "^15.0.9",
    "prop-types": "^15.7.2",
    "react-i18next": "^10.6.2"
  },
  "peerDependencies": {
    "gatsby": ">2.3.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "gatsby": "^2.0.31",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  }
}