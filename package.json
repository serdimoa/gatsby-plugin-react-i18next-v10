{
  "name": "gatsby-plugin-react-i18next-v10",
  "version": "0.0.1",
  "description": "Gatsby plugin that provides i18n support with react-i18next@v10",
  "main": "index.js",
  "author": "SerDimoa",
  "license": "MIT",
  "keywords": [
    "react",
    "gatsby",
    "gatsby-plugin",
    "gatsby-component",
    "i18next",
    "i18n"
  ],
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "build:watch": "babel -w src --out-dir . --ignore __tests__",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@wapps/gatsby-i18n": "^1.1.3",
    "i18next": "^15.0.9",
    "prop-types": "^15.7.2",
    "react-i18next": "^10.6.2"
  },
  "peerDependencies": {
    "gatsby": ">2.3.16"
  },
  "devDependencies": {
    "gatsby": "^2.0.31",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  }
}
